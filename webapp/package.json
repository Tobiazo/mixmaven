{
  "name": "webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier '**/*.{ts,tsx,js,css,md,json}' --write",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:ff": "cypress open --e2e --browser firefox",
    "cy:4173": "cypress run --env CLIENT_PORT=4173",
    "cy:prod": "npm run build && (npm-run-all -p -r preview cy:4173)",
    "server:start": "cd ../json-server && npm run start",
    "all": "npm-run-all -p -r server:start dev cy:ff"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@formkit/auto-animate": "^0.8.0",
    "@mui/icons-material": "^5.14.14",
    "@mui/material": "^5.14.14",
    "@tanstack/react-query": "^5.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loading": "^2.0.3",
    "react-router-dom": "^6.17.0",
    "react-uuid": "^2.0.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.7",
    "@tanstack/react-query-devtools": "^5.1.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-query": "^1.2.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "cypress": "^13.4.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  }
}
